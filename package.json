{
  "name": "electron-vue-template",
  "version": "0.1.0",
  "main": "electron/main.ts",
  "author": {
    "name": "korealu",
    "email": "korealu@foxmail.com"
  },
  "description": "electron-打包",
  "build": {
    "appId": "lsl.first.app",
    "productName": "A梦+",
    "copyright": "Copyright © 2023 lsl",
    "directories": {
      "output": "dist_electron"
    },
    "win": {
      "icon": "./build/electron-icon/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "icon": "./build/electron-icon/icon.png",
      "target": "AppImage"
    },
    "mac": {
      "icon": "./build/electron-icon/icon.icns"
    },
    "files": [
      "./dist",
      "./electron",
      "!**/node_modules/**"
    ],
    "asar": false,
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/electron-icon/icon.ico",
      "uninstallerIcon": "./build/electron-icon/icon.ico",
      "installerHeaderIcon": "./build/electron-icon/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "dev": "concurrently \"wait-on http://localhost:9999 && electron .\" \"cross-env BROWSER=none npm start\"",
    "build": "vue-cli-service build",
    "build:electron": "vue-cli-service build && electron-builder",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "commit": "cz",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "axios": "^1.2.0-alpha.1",
    "concurrently": "^8.2.0",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "element-plus": "^2.3.7",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.3",
    "underscore": "^1.13.6",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/underscore": "^1.11.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.0",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "prettier": "^2.8.8",
    "typescript": "~4.5.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
